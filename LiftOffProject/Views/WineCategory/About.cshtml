@model List<LiftOffProject.Models.WineCategory>

@if (Model.Count == 0)
{
    <h1>No Wine Categories Listed</h1>
}
else
{
      <h1>Wine Categories in the Database</h1>

    <ul>
        @foreach (var category in Model)
        {
            <li>Category: @category.Name , Flavors: @category.Flavors</li>
            
        }
    </ul>

}